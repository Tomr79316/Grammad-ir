#, c-format
msgid "%s: warning: check size of %s: %d?\n"
msgstr "%s: сануулга: %s-н хэмжээг шалгах: %d?\n"

#: cuardach.c:125 cuardach.c:166 cuardach.c:247
#, c-format
msgid "%s: warning: problem closing %s\n"
msgstr "%s: сануулга: %s асуудал хаагдав.\n"

#: cuardach.c:453
#, c-format
msgid "%s: no grammar codes: %s\n"
msgstr "%s: дүрмийн код алга: %s\n"

#: cuardach.c:581
#, c-format
msgid "problem with the `cuardach' command\n"
msgstr "`cuardach' тушаалд асуудал гарав\n"

#: cuardach.c:591
#, c-format
msgid "%s: problem reading the database\n"
msgstr "%s: өгөгдлийн бааз уншиж байхад алдаа\n"

#: farc.c.in:46
#, c-format
msgid "%s: illegal grammatical code\n"
msgstr "%s: хүчингүй дүрэмтэй код\n"

