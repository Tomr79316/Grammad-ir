#, c-format
msgid "Line %d: %s\n"
msgstr "Riadok %d: %s\n"

#: gram.pl:165 gramdev.pl:82
#, perl-format
msgid "unrecognized option %s"
msgstr "neznáma voľba %s"

#: gram.pl:168
